{
	"functions": {
		"source": "functions",
		"runtime": "nodejs18"
	},
	"hosting": {
		"public": "dist",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "/hello",
				"function": "helloWorld"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}
